hangeName = (name: string) => {\r\n        setActivePage(0);\r\n        setName(name);\r\n    }\r\n\r\n    const handleChangeCategory = (category: Category) => {\r\n        setActivePage(0);\r\n        setCategory(category);\r\n    }\r\n\r\n    const clearFilters = () => {\r\n        setActivePage(0);\r\n        setCategory(undefined);\r\n        setName('');\r\n     }\r\n    return (\r\n        <div className=\"catalog-container\">\r\n            <div className=\"d-flex justify-content-between\">\r\n                <h1 className=\"catalog-title\">\r\n                    Catálogo de produtos\r\n                </h1>\r\n                <ProductFilters \r\n                    name={name}\r\n                    category={category}\r\n                    handleChangeCategory={handleChangeCategory}\r\n                    handleChangeName={handleChangeName}\r\n                    clearFilters={clearFilters}\r\n                />\r\n            </div>\r\n            <div className=\"catalog-products\">\r\n                {isLoading ? <ProductCardLoader /> : (\r\n                    productsResponse?.content.map(product => (\r\n                        <Link to={`/products/${product.id}`} key={product.id}>\r\n                            <ProductCard product={product} />\r\n                        </Link>\r\n                    ))\r\n                )}\r\n            </div>\r\n            {productsResponse && (\r\n                <Pagination \r\n                    totalPages={productsResponse?.totalPages} \r\n                    activePage={activePage}\r\n                    onChange={page => setActivePage(page)}\r\n                />\r\n            )}\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Catalog;",["155","156"],"D:\\APPS\\ws-bootcamp\\dscatalog\\front-web\\src\\Routes.tsx",[],"D:\\APPS\\ws-bootcamp\\dscatalog\\front-web\\src\\pages\\Home\\index.tsx",[],["157","158"],"D:\\APPS\\ws-bootcamp\\dscatalog\\front-web\\src\\core\\components\\ButtonIcon\\index.tsx